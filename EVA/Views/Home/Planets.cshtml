@model PlanetsViewModel

<div class="page-heading">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h6>@Model.WelcomeMessage</h6>
                <h2>@Model.SolarName</h2>
                <span>Home > <a href="planets.html">@Model.PlanetsTitle</a></span>
            </div>
        </div>
    </div>
    <div class="featured-explore">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="owl-features owl-carousel">

                        <div class="item">
                            <div class="thumb">
                                <img src="assets/images/mercury.jpg" alt="pic" style="border-radius: 20px;">
                                <div class="hover-effect">
                                    <div class="content">
                                        <h4>@Model.MercuryName</h4>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="item">
                            <div class="thumb">
                                <img src="assets/images/venus.jpg" alt="pic" style="border-radius: 20px;">
                                <div class="hover-effect">
                                    <div class="content">
                                        <h4>@Model.VenusName</h4>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="item">
                            <div class="thumb">
                                <img src="assets/images/earth.jpg" alt="" style="border-radius: 20px;">
                                <div class="hover-effect">
                                    <div class="content">
                                        <h4>@Model.EarthName</h4>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="item">
                            <div class="thumb">
                                <img src="assets/images/mars.jpg" alt="" style="border-radius: 20px;">
                                <div class="hover-effect">
                                    <div class="content">
                                        <h4>@Model.MarsName</h4>
                                    </div>
