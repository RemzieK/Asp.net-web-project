        <div class="modal-content">

            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-12">
                        <div class="modal-body">
                            <!-- Project details-->
                            <h2 class="text-uppercase">@Model.MercuryName</h2>
